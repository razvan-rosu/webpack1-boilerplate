{
  "name": "webpack1-boilerplate",
  "version": "0.0.1",
  "description": "a webpack version 1.* boilerplate for web projects",
  "scripts": {
    "start": "npm run clean && NODE_ENV=production webpack-dev-server",
    "serve": "webpack-dev-server",
    "build:dev": "npm run clean && NODE_ENV=development webpack --watch",
    "build:prod": "npm run clean && NODE_ENV=production webpack",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/razvan-rosu/webpack1-boilerplate.git"
  },
  "keywords": [
    "webpack 1.*",
    "boilerplate",
    "es6",
    "es5 transpile",
    "bundles",
    "scss",
    "css autoprefixer"
  ],
  "author": "Răzvan Roșu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/razvan-rosu/webpack1-boilerplate/issues"
  },
  "homepage": "https://github.com/razvan-rosu/webpack1-boilerplate#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^11.3.6",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^1.15.0",
    "webpack-dev-server": "^1.16.5"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "mithril": "^1.1.6",
    "mithril-router": "^1.3.3",
    "moment": "^2.22.2",
    "node-normalize-scss": "^8.0.0"
  }
}
